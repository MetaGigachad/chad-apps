{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vite/client", "jest"],
    "noEmit": true,
    "isolatedModules": true,
  },
  "exclude": ["src/api/*.ts", "src/api/api.ts"],
}
